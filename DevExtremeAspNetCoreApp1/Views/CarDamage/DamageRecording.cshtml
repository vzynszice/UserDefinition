@using DAL.Enums
@using DAL.Models
@{
    ViewData["Title"] = "Car Damage Assessment";
    Layout = "_Layout";
    var damageTypes = Enum.GetValues(typeof(DamageType)).Cast<DamageType>();
    var partTypes = Enum.GetValues(typeof(PartType)).Cast<PartType>();
    var damageTypeNames = Enum.GetValues(typeof(DamageType))
        .Cast<DamageType>()
        .ToDictionary(x => (int)x, x => x.ToString());

    var partTypeNames = Enum.GetValues(typeof(PartType))
        .Cast<PartType>()
        .ToDictionary(x => (int)x, x => x.ToString());
   
    var DAMAGE_COLORS = new Dictionary<int, string> {
        {(int)DamageType.Original, "#FFFFFF"},      // Orijinal parça - Beyaz
        {(int)DamageType.Crushed, "#FF6B6B"},      // Ezik parça - Kırmızı
        {(int)DamageType.Scratch, "#4ECDC4"},      // Çizik parça - Turkuaz
        {(int)DamageType.Partialrepaint, "#FFD93D"},// Lokal boyalı - Sarı
        {(int)DamageType.Repainted, "#95DAC1"},    // Boyalı parça - Açık Yeşil
        {(int)DamageType.Replaced, "#6C5B7B"}      // Değişen parça - Mor
    };
}
<input type="hidden" id="carData" value='@Html.Raw(ViewBag.CarData)' />
<div class="damage-assessment-container">
    <div class="header-section">
        <h2>Car Damage Assessment</h2>
    </div>

    <div class="content-wrapper">
        <div class="grid-container">
            @(Html.DevExtreme().DataGrid<dynamic>()
                .ID("damageGrid")
                .DataSource(d => d.Array()
                .Data(new object[] { }) // Boş array ile başlatıyoruz, JavaScript'te dolduracağız
                .Key("partTypeId")
                )
                .RemoteOperations(false)
                .ShowBorders(true)
                .ShowRowLines(true)
                .Paging(p=>p.Enabled(false))
                .ShowColumnLines(true)
                .Width("100%")
                .Height("auto")
                .RowAlternationEnabled(true)  // Makes it easier to distinguish rows
                .NoDataText("No data to display")
                .Selection(s => s
                .Mode(SelectionMode.Single)
                )
                .Editing(e => e
                .AllowUpdating(true)
                .Mode(GridEditMode.Cell)
                )
                .OnSelectionChanged("onGridSelectionChanged")
                .OnCellClick("onGridCellClick")
                .LoadPanel(p => p
                .Enabled(true)
                .ShowIndicator(true)
                .ShowPane(true)
                .Text("Loading...")
                )
                .Columns(columns =>
                {
                    columns.Add()
                    .Caption("Car Parts / Damages")
                    .DataField("partType")
                    .AllowEditing(false)
                    .Width(200);

                    foreach (DamageType damage in damageTypes)
                    {
                        columns.Add()
                        .Caption(damage.ToString())
                        .DataField($"damage_{(int)damage}")
                        .DataType(GridColumnDataType.Boolean)
                        .Width(100)
                        .Alignment(HorizontalAlignment.Center)
                        .AllowEditing(true);
                    }
                })
                )
        </div>

        
        <div class="car-svg-container">
            <div class="car-visualization">
                
                <div class="damage-legend">
                    @foreach (DamageType damage in damageTypes)
                    {
                        <div class="legend-item">
                            <div class="legend-color" style="background-color: @DAMAGE_COLORS[(int)damage]"></div>
                            <span class="legend-name">@damage.ToString()</span>
                        </div>
                    }
                </div>

